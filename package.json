{
  "name": "tree",
  "description": "full-feature tree component",
  "keywords": [
    "tree",
    "ui"
  ],
  "version": "0.0.0",
  "author": {
    "name": "e-cloud",
    "email": "saintscott119@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "lodash": "^4.14.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.16.0",
    "ghooks": "^1.3.1",
    "karma": "^1.1.2",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-notify-reporter": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.0",
    "sinon": "^1.17.5",
    "standard-version": "^2.4.0",
    "validate-commit-msg": "^2.8.0",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "lint": "eslint src/**/*",
    "test": "karma start",
    "release": "standard-version"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "perf",
        "docs",
        "style",
        "refactor",
        "example",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 72,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "\n# allowed type: feat, fix, perf, docs, style, refactor, example, test, chore, revert\n# subject no more than 50 chars\n# a body line no more than 72 chars"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg",
      "post-commit": "git log -1 HEAD",
      "post-checkout": "npm install",
      "post-merge": "npm install"
    }
  }
}
